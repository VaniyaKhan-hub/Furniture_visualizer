<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="../admin/assets/images/logo.png">
    <title>admin dashboard</title>
    <!-- Custom CSS -->
    <link href="../admin/dist/css/style.min.css" rel="stylesheet">
    <!-- This Page CSS -->
    <link href="../admin/assets/libs/morris.js/morris.css" rel="stylesheet">
</head>
<body>
     <!-- ============================================================== -->
    <!-- Preloader - style you can find in spinners.css -->
    <!-- ============================================================== -->
    <div class="preloader">
        <div class="lds-ripple">
            <div class="lds-pos"></div>
            <div class="lds-pos"></div>
        </div>
    </div>
    <!-- ============================================================== -->
    <!-- Main wrapper - style you can find in pages.scss -->
    <!-- ============================================================== -->
    <div id="main-wrapper" data-theme="light" data-layout="vertical" data-navbarbg="skin6" data-sidebartype="full" data-sidebar-position="fixed" data-header-position="fixed" data-boxed-layout="full">
     
        <!-- header -->
        <%- include("./parts/header.ejs"); %>
       
        <!-- sidebar -->
         <%- include("./parts/sidebar.ejs"); %>
         
        <!-- ============================================================== -->
        <!-- Page wrapper  -->
        <!-- ============================================================== -->
         <div class="page-wrapper">
        <section class="dash">
            <h2>Products</h2>
            <a href="/dash/addproduct" class="add-btn">Add Products</a>

            <div class="dash-table">

                <table class="dash-tab">
                    <thead>
                        <tr>
                            <th>Id</th>
                            <th>Image1</th>
                            <th>Image2</th>
                            <th>Image3</th>
                            <th>Title</th>
                            <th>price</th>
                            <!-- <th>Description</th>
                            <th>Specifications</th> -->
                            <th>Dimensions</th>
                            <th>Material</th>
                            <th>Actions</th>
                        </tr>
                    </thead>

                    <tbody>
                        <% let i = 0 %>

                        <% allprod.forEach((product)=>{ %>

                            <% i++ %>
                        <tr>
                            <td><%= i %></td>
                            <td>
                                <img src="/assets/<%= product.Image1 %>" alt="image1">
                            </td>
                            <td>
                                <img src="/assets/<%= product.Image2 %>" alt="">
                            </td>
                            <td>
                                <img src="/assets/<%= product.Image3 %>" alt="">
                            </td>
                            <td><%= product.Title %></td>
                            <td><%= product.Price %></td>
                            <!-- <td><%= product.Description %></td>
                            <td><%= product.Specifications %></td> -->
                            <td><%= product.Dimensions %></td>
                            <td><%= product.Material %></td>
                            <td class="actionbtn">
                                <a class="btn-edit" href="/dash/editproduct/<%= product._id %>">Edit</a>
                                <a class="btn-delete" href="/dash/deleteproduct/<%= product._id %>">Delete</a>
                            </td>
                        </tr>
                        <% }) %>
                    </tbody>
                </table>
            </div>
        </section>
             </div>
           
            <!-- footer -->
           
            <footer class="footer text-center text-muted">
                All Rights Reserved by Roomify
            </footer>
    </div>
    <!-- All Jquery -->
    <!-- ============================================================== -->
    <script src="../admin/assets/libs/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap tether Core JavaScript -->
    <script src="../admin/assets/libs/popper.js/dist/umd/popper.min.js"></script>
    <script src="../admin/assets/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <!-- apps -->
    <!-- apps -->
    <script src="../admin/dist/js/app-style-switcher.js"></script>
    <script src="../admin/dist/js/feather.min.js"></script>
    <!-- slimscrollbar scrollbar JavaScript -->
    <script src="../admin/assets/libs/perfect-scrollbar/dist/perfect-scrollbar.jquery.min.js"></script>
    <script src="../admin/assets/extra-libs/sparkline/sparkline.js"></script>
    <!--Wave Effects -->
    <!-- themejs -->
    <!--Menu sidebar -->
    <script src="../admin/dist/js/sidebarmenu.js"></script>
    <!--Custom JavaScript -->
    <script src="../admin/dist/js/custom.min.js"></script>
    <!-- This Page JS -->
    <!--Morris JavaScript -->
    <script src="../admin/assets/libs/raphael/raphael.min.js"></script>
    <script src="../admin/assets/libs/morris.js/morris.min.js"></script>
    <script src="../admin/dist/js/pages/morris/morris-data.js"></script>
</body>
</html>